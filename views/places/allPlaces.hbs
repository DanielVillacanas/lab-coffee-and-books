
<div class = "allPlaces">
  <h1>All Places</h1>
<div class="container">
  <div class="row cardsAll">
  {{#each allPlaces}}
    <div class="card col-md-5">
      <h3>Name : {{name}}</h3>
      <p>Type : {{type}}</p>
    <div class="buttons">
      <a class="btn btn-dark" href="/places/editPlace/{{_id}}">Edit Place</a>
      <a class="btn btn-dark" href="/places/delete/{{_id}}">Delete Place</a>
    </div>
    </div>
  {{/each}}
  </div>
</div>
  <div id="map"></div>
  <div class="newElement">
  <a class="btn btn-primary" href="/places/create">Create a new Place</a>
  </div>
</div>

